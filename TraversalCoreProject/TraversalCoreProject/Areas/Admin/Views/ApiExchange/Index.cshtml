@model IEnumerable<TraversalCoreProject.Areas.Admin.Models.ExchangeModel2.Exchange_Rates>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Döviz Kuru</h1>
<table  class="table table-striped">
    <tr>
        <th>Döviz Cinsi</th>
        <th>Kur Oranı</th>
    </tr>

    @foreach (var item in Model)
    {

        <tr>
            <td>@item.currency</td>
            <td>@item.exchange_rate_buy</td> 
        </tr>
        }
</table>
